{
  "name": "react-starter",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.4.1",
  "description": "My React template",
  "author": "Talljack <yugang.cao12@gmail.com> (https://github.com/talljack)",
  "license": "MIT",
  "funding": "https://github.com/sponsors/talljack",
  "homepage": "https://github.com/talljack/react-starter",
  "bugs": {
    "url": "https://github.com/talljack/react-starter/issues"
  },
  "keywords": [
    "React",
    "React Template"
  ],
  "main": "index.ts",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks",
    "lint": "eslint --fix --cache .",
    "format": "prettier --write --cache .",
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "test": "vitest",
    "test:cov": "vitest run --coverage"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@headlessui/tailwindcss": "^0.2.2",
    "@heroicons/react": "^2.2.0",
    "@tanstack/react-router": "^1.114.23",
    "classnames": "^2.5.1",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "immer": "^10.1.1",
    "jotai": "^2.12.2",
    "jotai-devtools": "^0.11.0",
    "jotai-immer": "^0.4.1",
    "next-i18next": "^15.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-use": "^17.6.0",
    "use-immer": "^0.11.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.11",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.9",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "esno": "^4.8.0",
    "happy-dom": "^17.4.4",
    "lint-staged": "^15.5.0",
    "picocolors": "^1.1.1",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.5.3",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^4.0.15",
    "talljack-eslint-config": "^0.2.6",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.9"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "npx esno scripts/verifyCommit.ts"
  },
  "lint-staged": {
    "*": "eslint . --fix"
  }
}
