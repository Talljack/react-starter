{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "My React template",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "main": "index.ts",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks",
    "lint": "eslint --cache .",
    "format": "prettier --write --cache .",
    "dev": "vite",
    "build": "tsc --noEmit && vite build"
  },
  "dependencies": {
    "@tanstack/react-router": "0.0.1-beta.83",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.2",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esno": "^0.16.3",
    "lint-staged": "^13.2.0",
    "picocolors": "^1.0.0",
    "prettier": "^2.8.7",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vite-tsconfig-paths": "^4.0.7"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "esno scripts/verifyCommit.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache",
      "prettier --write --cache"
    ],
    "*.{json,md,css,scss,html}": [
      "prettier --write --cache"
    ]
  }
}
