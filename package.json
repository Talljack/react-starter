{
  "name": "react-starter",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.12.3",
  "description": "My React template",
  "author": "Talljack <yugang.cao12@gmail.com> (https://github.com/talljack)",
  "license": "MIT",
  "funding": "https://github.com/sponsors/talljack",
  "homepage": "https://github.com/talljack/react-starter",
  "bugs": {
    "url": "https://github.com/talljack/react-starter/issues"
  },
  "keywords": [
    "React",
    "React Template"
  ],
  "main": "index.ts",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks",
    "lint": "eslint --fix --cache .",
    "format": "prettier --write --cache .",
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "test": "vitest",
    "test:cov": "vitest run --coverage"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@headlessui/tailwindcss": "^0.2.1",
    "@heroicons/react": "^2.1.5",
    "@tanstack/react-router": "^1.78.3",
    "classnames": "^2.5.1",
    "i18next": "^23.16.4",
    "i18next-browser-languagedetector": "^8.0.0",
    "immer": "^10.1.1",
    "jotai": "^2.10.1",
    "jotai-devtools": "^0.10.1",
    "jotai-immer": "^0.4.1",
    "next-i18next": "^15.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.1.0",
    "react-use": "^17.5.1",
    "use-immer": "^0.10.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.8.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "esno": "^4.8.0",
    "happy-dom": "^15.9.0",
    "lint-staged": "^15.2.10",
    "picocolors": "^1.1.1",
    "postcss": "^8.4.47",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.3.3",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.14",
    "talljack-eslint-config": "^0.2.6",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "npx esno scripts/verifyCommit.ts"
  },
  "lint-staged": {
    "*": "eslint . --fix"
  }
}
